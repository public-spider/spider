<script>
(function(){
	var uids = [];
	$('[uid]').each(function(){
		uids.push($(this).attr('uid'));
	})
	var url = "http://music.weibo.com/snake/port/snk_counts.php";
	var formatNum = function(num){
		return num > 10000 ? Math.ceil(num / 10000)+'万' : num;
	}
	$.post(url,{singerids:uids},function(data){
		var lists = data.data;
		for(var i in lists){
			$("[uid="+i+"]").find('i').eq(0).text(formatNum(lists[i].popular_play_times));
			$("[uid="+i+"]").find('i').eq(1).text(lists[i].popular_fav_num);
		}
	},'json')
})()
</script>
