			<script type="text/javascript">
				(function(){
					var navIndex ={'mobile':0,'msg':1,'iphone':2,'ipad':3,'android':4,'android_pad':5,'symbian':6,'kjava':7,'blackberry':8,'windowsphone':9,'win8':10,'mtk':11,'h5iphone':12,'h5ipad':13,'h5radio':14};
					if(window.location.hash){
						var BdivWrapper = eleChildFilter(document.getElementById('B_mobi_wrapper').childNodes);
						var Bnav = eleChildFilter(document.getElementById('left_havSC_menus').childNodes);
						Bnav[0].className='';
						BdivWrapper[0].id='';
						BdivWrapper[navIndex[window.location.hash.substring(1)]].id='B_mobi_current';
						Bnav[navIndex[window.location.hash.substring(1)]].className = 'current';
					}
					function eleChildFilter(childA){
						var eleChildA = [],index=0;
						for(var i=0;i<childA.length;i++){
							if(childA[i].nodeType === 1){
								eleChildA[index] = childA[i];
								index++;
							}
						}
						return eleChildA;
					}
				})();
			</script>
		
