<!DOCTYPE HTML>
<html>
<head>
<meta charset="utf-8">
<title>音乐人播放器</title>
<link rel="stylesheet" type="text/css" href="http://music.weibo.com/snake/css/snk_player.css"/>
<link rel="stylesheet" type="text/css" href="http://music.weibo.com/snake/css/snk_layer.css?v=20130423"/>
</head>

<body>
<script type="text/javascript" src="/t/js/wbmusicplayer.js?version=20120425"></script>
<script type="text/javascript">
	wbmusicplayer.initPlayObj({openLc:true,flash_object:'http://music.sina.com.cn/yueku/js/mwp/radio_utils.swf?v=20130426',flashVar : {lib_coflag : ""}});
	var init_val = {
		songids : "",
		shortUrl : "",
		radioId : "",
		coflag : "0"||"500013"
	}
</script>

<!--播放器列表begin-->
<div class="playerMain">
    <div class="mainWapper">
        <div class="listLeft" id="listleft">
          <h2 class="clearfix" node-id="history_0">
              <a href="javascript:;" node-act="radiolist" node-data="type=history&id=0">
                  <span class="icon_list FL"></span>
                  <span class="FL text">临时播放列表</span>
                  <span class="arrow FR"></span>
				  <span class="icon_listening none" style="display:none;" node-id="listening"></span>
              </a>
          </h2>
          <dl class="playitem">
            <dt>
                <div class="clearfix">
                    <span class="icon_hot FL"></span>
                    <span class="FL">热门推荐</span>
                </div>
            </dt>
            <dd node-id="public_4"><a href="javascript:;" node-act="radiolist" node-data="type=public&id=4"><span class="FL text">24小时热歌排行榜</span><span class="arrow FR"></span><span class="icon_listening none" style="display:none;" node-id="listening"></span><span class="loading" node-id="loading"></span></a></dd>
            <dd node-id="feature_153"><a href="javascript:;" node-act="radiolist" node-data="type=feature&id=153"><span class="FL text">流行频率</span><span class="arrow FR"></span><span class="icon_listening none" style="display:none;" node-id="listening"></span><span class="loading" node-id="loading"></span></a></dd>
            <dd node-id="feature_154"><a href="javascript:;" node-act="radiolist" node-data="type=feature&id=154"><span class="FL text">民谣频率</span><span class="arrow FR"></span><span class="icon_listening none" style="display:none;" node-id="listening"></span><span class="loading" node-id="loading"></span></a></dd>
            <dd node-id="feature_156"><a href="javascript:;" node-act="radiolist" node-data="type=feature&id=156"><span class="FL text">电子频率</span><span class="arrow FR"></span><span class="icon_listening none" style="display:none;" node-id="listening"></span><span class="loading" node-id="loading"></span></a></dd>
            <dd node-id="feature_155"><a href="javascript:;" node-act="radiolist" node-data="type=feature&id=155"><span class="FL text">摇滚频率</span><span class="arrow FR"></span><span class="icon_listening none" style="display:none;" node-id="listening"></span><span class="loading" node-id="loading"></span></a></dd>
            <dd node-id="feature_232"><a href="javascript:;" node-act="radiolist" node-data="type=feature&id=232"><span class="FL text">行乐频率</span><span class="arrow FR"></span><span class="icon_listening none" style="display:none;" node-id="listening"></span><span class="loading" node-id="loading"></span></a></dd>
            </dl>
          <dl class="playitem">
            <dt>  
            <div class="clearfix">
                    <span class="icon_heart FL"></span>
                    <span class="FL">私人曲库</span>
             </div>
            </dt>
            <dd node-id="private_2"><a href="javascript:;" node-act="radiolist" node-data="type=private&id=2"><span class="FL text">我喜欢的歌</span><span node-id="count" class="none">（<i node-id="favsongnum"></i>首）</span><span class="arrow FR"></span><span class="icon_listening none" style="display:none;" node-id="listening"></span><span class="loading" node-id="loading"></span></a></dd>
            <dd node-id="private_1"><a href="javascript:;" node-act="radiolist" node-data="type=private&id=1"><span class="FL text">我喜欢的音乐人</span><span node-id="new" class="none">（<i node-id="favartistnum"></i>首）</span><i class="new none" node-id="new">new</i><span class="arrow FR"></span><span class="icon_listening none" style="display:none;" node-id="listening"></span><span class="loading" node-id="loading"></span></a></dd>
        </div>
        <!--右侧歌词begin-->
        <div class="listRight" id="lyricsarea">
                <h2>歌词</h2>
                <div class="pic clearfix">
                    <div class="pic_img FL"><a href="" target="_blank"><img src="http://image2.sina.com.cn/music/web/weibo2012/base/default.jpg" width="86" height="86" node-id="albumimg"><div class="light"></div></a>
					</div>
                    <a href="javascript:;" class="shareLyric FL unshare" node-id="share"></a><!--样式unshare为不能分享状态，去掉样式为正常-->
                </div>
                <div class="LyricText" node-id="lyrics_container">
                    <!--没有歌词状态-->
                    <P class="unLyricText none" node-type="nolyrics">抱歉，音乐人未上传歌词</P>
                    <!--歌词-->
                    <div class="songName" node-id="songName" node-type="haslyrics"></div>
                    <div class="LyricFont" node-id="lyrics_view" node-type="haslyrics">
						<dl node-id="lyrics">
							
						</dl>
                    
                   </div> 
                </div>
            </div>
        <!--右侧歌词end-->
        <div class="listCenter">
               <div class="listwapperA scrolling" node-id="list">
                <!--歌曲列表begin-->
                <div class="listwapperB">
                    <div class="listWapperC">
                    <!--歌曲列表头部begin-->
                    <div class="playList listTitle">
                       <div class="songName">
                        <span class="num"></span>
                        <span>歌曲</span>
                       </div>
                       <div class="artist">艺人</div>
                       <div class="listIcon">
                       </div>
                     </div>
                    <!--歌曲列表头部end-->
                    <!--歌曲列表begin-->
                    <div class="listWapperD" id="songlist_view">
                     
                     </div>
                    <!--歌曲列表end--> 
                    </div>
                </div>
                <!--歌曲列表end-->
                <!--滚动条begin-->
                <div class="scrollBar" id="barcontainer">
                    <a href="javascript:;" node-id="pbar">
                        <span class="scrollTop"></span>
                        <span class="scrollCen" id="scrollbar"></span>
                        <span class="scrollBt"></span>
                    </a>
               </div>
               <!--滚动条end-->
               
               </div>
               <!--清除列表begin-->
              <div class="clearList"><a href="javascript:;" class="clearListBtn unclear" id="clearlist"></a></div> 
               <!--清除列表end-->
			   <!--没有歌曲 begin-->
               <div class="list_empty none" node-id="nolist">
               </div>
               <!--没有歌曲 end-->
            </div>
    </div>
</div>
<!--播放器列表end-->
<!--播放器begin-->
<div class="panelWapper">
    <div class="panel">
        <!--播放按钮begin-->
        <div class="panelLeft">
            <ul class="playBtn" id="playbtn">
                <li><a href="javascript:;" class="prev" node-act="prev"></a></li>
                <li><a href="javascript:;" class="play" node-act="play"></a></li><!--播放 暂停按钮-->
                <li><a href="javascript:;" class="next" node-act="next"></a></li>
            </ul>
         </div>
        <!--播放按钮end-->
        <!--分享 收藏 声音等控制按钮begin-->
        <div class="panelRight">
            <!--分享-->
            <ul class="shareList" id="othercontrol">
                <li><a href="javascript:;" class="share" node-act="share" title="分享到微博"></a></li>
                <li><a href="javascript:;" class="collect" node-act="collect" node-id="collect" title="喜欢这首歌"></a></li><!--cur为收藏状态-->
            </ul>
            <!--声音-->
            <div class="playSound" id="volume">
                <a href="javascript:;" class="sound" node-id="btn_volume"></a><!--unsound为静音状态-->
                <div class="soundProgress">   
                    <div class="progress">
                         <div class="progressBg" node-id="bg_volume"></div><!--进度背景层-->
                         <div class="progressJd" node-id="cur_volume"></div><!--进度条播放层-->
						 <a href="javascript:;" class="progressBar" node-id="bar_volume"></a>
                     </div>
                </div>
            </div>
            <!--播放模式-->
            <ul class="playMode" id="playmode">
                <li class="cur"><a href="javascript:;" class="moreR" node-act="playmode" node-data="loop"></a></li><!--cur为当前状态-->
                <li><a href="javascript:;" class="oneR" node-act="playmode" node-data="one"></a></li>
            </ul>
        </div>
        <!--分享 收藏 声音等控制按钮end-->
        <!--进度条 begin-->
        <div class="panelMain" id="playbox">
         <div class="panel_msg">
            <span><a href="javascript:;" target="_blank" class="songName" node-id="songname"></a><a href="javascript:;" class="singer" node-id="singername"></a></span>
         </div>
         <div class="progressWapper">
             <div class="progress">
                 <div class="progressBg" node-id="bg"></div><!--进度背景层-->
                 <div class="progressLd" node-id="load"></div><!--进度条加载层-->
                 <div class="progressJd" node-id="position"></div><!--进度条播放层-->
				 <a href="javascript:;" class="progressBar" node-id="positionbar"></a>
             </div>
             <div class="time" node-id="time">-00:00</div><!--歌曲时间-->
         </div>
        </div> 
        <!--进度条 end-->
        <!--提示 begin-->
         <div class="cuebox">上次播到这儿</div>
        <!--提示 end--> 
    </div>
</div>
<!--播放器end-->
<!--头部begin-->
<div class="playerHd">
<div class="enter" id="top_user">还没有微博帐号？<a href="http://weibo.com/signup/signup.php" target="_blank">注册</a><span class="line">|</span><a href="javascript:void(0);" node-act="login">登录</a></div>
<div class="logo"><a href="http://music.weibo.com" target="_blank"><img src="http://i2.sinaimg.cn/music/web/weibo2012/base/transparent.gif"></a></div>
 
</div>
<!--头部end-->
<script type="text/javascript" src="/snake/js/jquery.1.7.min.js" ></script>
<script type="text/javascript" src="http://js.t.sinajs.cn/t5/register/js/page/remote/loginLayer.js"></script>
<script type="text/javascript" src="/t/js/lyricsTools.js" ></script>
<script type="text/javascript" src="/snake/js/jquery-ui.js"></script>
<script type="text/javascript" src="/snake/js/snk_player.js?v=20130520" ></script>
<script type="text/javascript" src="/snake/js/snk_layer.js" ></script>
</body>
</html>  