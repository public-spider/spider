<script>
        $(document).ready(function(){
            $.getScript('http://js.t.sinajs.cn/t5/pack/js/independent/musicplayer_3fc58e120c6d3f29.js',function(){
                  WEIBO_MUSIC_PLAYER.setUid('5060400438');
                  WEIBO_MUSIC_PLAYER.on('like', function (evt, song, callback) {
                    if(!song.zan.is_zan){
                        $.post("http://ting.weibo.com/music/ajax/like",{songid:song.songid});
                    }else{
                        $.post("http://ting.weibo.com/music/ajax/dislike",{songid:song.songid});
                    }
                        callback(!song.zan.is_zan);
                  });
                  WEIBO_MUSIC_PLAYER.on('share', function (evt, song) {
                      var id = song.songid;
                      var sinaid = '5060400438'||'';
                      var type = '';
                      var h=(screen.height-610)/2;
	                  var g=(screen.width-628)/2;
	                  window.open("http://ting.weibo.com/music/ajax/share?id="+id+"&type="+type+"&sinaid="+sinaid,'','toolbar=0,status=0,resizable=1,width=628,height=610,top='+h+',left='+g);
                  });
            });

        });
</script>
